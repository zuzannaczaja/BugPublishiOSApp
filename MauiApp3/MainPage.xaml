        <DatePicker WidthRequest="150" Margin="0, 100, 0, 20"></DatePicker>
        <Entry ReturnType="Search"></Entry>
        <TimePicker WidthRequest="150" Format="HH:mm"></TimePicker>
    </StackLayout>

</ContentPage>
